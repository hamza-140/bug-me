<% if can? :update, @bug %>
<div class="d-flex justify-content-center align-items-center">
    <h1>Edit Bug</h1>
</div>
<%= render 'form', update_check: true %>
<% else %>
  <p>You are not authorized to edit this bug.</p>
  <%= link_to "Return", projects_path ,class:"btn btn-primary mt-3"%> 

<% end %>
